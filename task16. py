x = int(input("Введите число:x ")) 
a = int(input("Введите число:a ")) 
b = int(input("Введите число:b ")) 
c = int(input("Введите число:c ")) 
R = int(input("Введите число:R ")) 
d = int(input("Введите число:d ")) 
 
print(f"а) a / b / c= {a / b / c}") 
print(f"б) a * b / c = {a * b / c}") 
print(f"в) a / b * c = {a / b * c}") 
print(f"г) a + (b / c) = {a + (b / c)}") 
print(f"д) (a + b) / c = {(a + b / c)}") 
print(f"е) a + b / b + c = {a + b / b + c}") 
print(f"ж) (a + b) / (b + c) = {(a + b) / (b + c)}") 
print(f"з) a / sin(b) = {a / sin (b)}") 
print(f"и) 1 / 2 * a * b sin(x) = {1 / 2 * a * b * sin(x)}") 
print(f"к) 2 * b * c * cos(a / 2) / b + c = {2 * b * c * cos(a / 2) / b + c}") 
print(f"л) 4 * R * sin(a / 2) * sin(b / 2) * sin(c / 2) \ 
    = {4 * R * sin(a / 2) * sin(b / 2) * sin(c / 2)}") 
print(f"м) (a * x + b) / (c * x + d) = {(a * x + b) / (c * x + d)}")